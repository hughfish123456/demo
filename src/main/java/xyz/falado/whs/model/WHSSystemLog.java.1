package xyz.falado.whs.model;

import java.io.Serializable;
import java.util.Date;

public class WHSSystemLog implements Serializable {
    private Long id;

    private Date createdate;

    private Integer createdby;

    private String message;

    private static final long serialVersionUID = 1L;

    public WHSSystemLog(Long id, Date createdate, Integer createdby) {
        this.id = id;
        this.createdate = createdate;
        this.createdby = createdby;
    }

    public WHSSystemLog(Long id, Date createdate, Integer createdby, String message) {
        this.id = id;
        this.createdate = createdate;
        this.createdby = createdby;
        this.message = message;
    }

    public WHSSystemLog() {
        super();
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Date getCreatedate() {
        return createdate;
    }

    public void setCreatedate(Date createdate) {
        this.createdate = createdate;
    }

    public Integer getCreatedby() {
        return createdby;
    }

    public void setCreatedby(Integer createdby) {
        this.createdby = createdby;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message == null ? null : message.trim();
    }
}