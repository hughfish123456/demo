package xyz.falado.whs.model;

import java.io.Serializable;
import java.util.Date;

public class WHSEmployeeLeave implements Serializable {
    private Integer id;

    private Integer employeeId;

    private Date leaveBegin;

    private Date leaveEnd;

    private static final long serialVersionUID = 1L;

    public WHSEmployeeLeave(Integer id, Integer employeeId, Date leaveBegin, Date leaveEnd) {
        this.id = id;
        this.employeeId = employeeId;
        this.leaveBegin = leaveBegin;
        this.leaveEnd = leaveEnd;
    }

    public WHSEmployeeLeave() {
        super();
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getEmployeeId() {
        return employeeId;
    }

    public void setEmployeeId(Integer employeeId) {
        this.employeeId = employeeId;
    }

    public Date getLeaveBegin() {
        return leaveBegin;
    }

    public void setLeaveBegin(Date leaveBegin) {
        this.leaveBegin = leaveBegin;
    }

    public Date getLeaveEnd() {
        return leaveEnd;
    }

    public void setLeaveEnd(Date leaveEnd) {
        this.leaveEnd = leaveEnd;
    }
}